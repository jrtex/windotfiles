{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/master/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/.config/komorebi/applications.json",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "mouse_follows_focus": true,
  "default_workspace_padding": 5,
  "default_container_padding": 5,
  "border": true,
  "border_colours": {
    "single": "#458588",
    "stack": "#98971a",
    "monocle": "#b16286",
    "unfocused": "#7c6f64",
    "floating": "#d79921"
  },
  "border_width": 5,
  "border_offset": -1,
  "border_implementation": "Komorebi",
  "display_index_preferences": {
    "0": "DELA0A4-7&1b958bf5&0&UID265",
    "1": "GSM77BD-7&1b958bf5&0&UID260",
    "2": "DELA0A4-7&1b958bf5&0&UID264"
  },
  "bar_configurations": [
    "$Env:USERPROFILE/.config/komorebi/komorebi.bar.tie_left.json",
    "$Env:USERPROFILE/.config/komorebi/komorebi.bar.4k.json",
    "$Env:USERPROFILE/.config/komorebi/komorebi.bar.tie_right.json"
  ],
  "monitors": [
    {
      "window_based_work_area_offset_limit": 1,
      "window_based_work_area_offset": {
        "left": 0,
        "top": 200,
        "right": 0,
        "bottom": 400
      },
      "workspaces": [
        { "name": "中", "layout": "Rows" },
        { "name": "东", "layout": "Rows" },
        { "name": "西", "layout": "Rows" },
        { "name": "北", "layout": "Rows" },
        { "name": "南", "layout": "Rows" }
      ]
    },
    {
      "window_based_work_area_offset_limit": 1,
      "window_based_work_area_offset": {
        "left": 325,
        "top": 180,
        "right": 650,
        "bottom": 360
      },
      "workspaces": [
        { "name": "一", "layout": "Grid" },
        { "name": "二", "layout": "Grid" },
        { "name": "三", "layout": "Grid" },
        { "name": "四", "layout": "Grid" },
        { "name": "五", "layout": "Grid" }
      ]
    },
    {
      "window_based_work_area_offset_limit": 1,
      "window_based_work_area_offset": {
        "left": 0,
        "top": 200,
        "right": 0,
        "bottom": 400
      },
      "workspaces": [
        { "name": "土", "layout": "Rows" },
        { "name": "火", "layout": "Rows" },
        { "name": "水", "layout": "Rows" },
        { "name": "木", "layout": "Rows" },
        { "name": "金", "layout": "Rows" }
      ]
    }
  ],
  "manage_rules": [
    {
      "kind": "Exe",
      "id": "ConEmu64.exe"
    },
    {
      "kind": "Title",
      "id": "\\| Microsoft Teams",
      "matching_strategy": "Regex"
    }
  ],
  "ignore_rules": [
    {
      "kind": "Exe",
      "id": "Zebar.exe",
      "matching_rules": "Equals"
    }
  ],
  "stackbar": {
    "height": 25,
    "label": "Process",
    "mode": "Never",
    "tabs": {
      "width": 300,
      "focused_text": "#98971a",
      "unfocused_text": "#928374",
      "background": "#282828"
    }
  },
  "animation": {
    "enabled": true,
    "duration": 120,
    "style": "EaseOutCirc"
  }
}
