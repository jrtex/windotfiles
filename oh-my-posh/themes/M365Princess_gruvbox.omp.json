{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "green": "#98971A",
    "green_dark": "#79740e",
    "purple_dark": "#8F3F71",
    "yellow": "#D79921",
    "blue_light": "#83A598",
    "tan": "#CC3802",
    "blue": "#458588",
    "white": "#FBF1C7",
    "red_dark": "#9D0006"
  },
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "left",
      "segments": [
        {
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": "{{ if .Env.PNPPSHOST }} \uf8c5 {{ .Env.PNPPSHOST }} {{ end }}",
          "foreground": "p:white",
          "background": "p:tan",
          "type": "text"
        },
        {
          "style": "powerline",
          "template": "{{ if .Env.PNPPSSITE }} \uf2dd {{ .Env.PNPPSSITE }}{{ end }}",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:blue",
          "type": "text"
        },
        {
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if .Env.PNPPSSITE }} {{ end }}",
          "foreground": "p:white",
          "background": "p:blue",
          "type": "text"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "diamond",
          "leading_diamond": "\u256d\ue0b6",
          "template": "{{ .UserName }} ",
          "foreground": "p:white",
          "background": "p:purple_dark",
          "type": "session"
        },
        {
          "properties": {
            "style": "full"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:green",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": "",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:yellow",
          "template": " ➜  ({{ .HEAD }}{{ if gt .Ahead 0 }} ↑{{ .Ahead }}{{ end }}{{ if gt .Behind 0 }} ↓{{ .Behind }}{{ end }}) <p:green_dark>{{ if gt .Staging.Modified 0 }} ~{{ .Staging.Modified }}{{ end }}{{ if gt .Staging.Added 0 }} +{{ .Staging.Added }}{{ end }}{{ if gt .Staging.Deleted 0 }} -{{ .Staging.Deleted }}{{ end }}</><p:red_dark>{{ if gt .Working.Modified 0 }} ~{{ .Working.Modified }}{{ end }}{{ if gt .Working.Untracked 0 }} ?{{ .Working.Untracked }}{{ end }}{{ if gt .Working.Deleted 0 }} -{{ .Working.Deleted }}{{ end }}</> ",
          "type": "git"
        },
        {
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:blue_light",
          "type": "node"
        },
        {
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "p:white",
          "background": "p:blue",
          "type": "time"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u2570\u2500",
          "foreground": "p:purple_dark",
          "type": "text"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " \ue285",
          "foreground": "p:white",
          "type": "status",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red_dark{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
